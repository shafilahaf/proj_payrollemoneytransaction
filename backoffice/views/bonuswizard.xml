<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="bo_bonus_wizard_view_form" model="ir.ui.view">
        <field name="name">bo.bonus.wizard.view.form</field>
        <field name="model">bo.bonus.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="uploadfile" />
                        <field name="bo_bonusheader_id" invisible="1"/>
                    </group>
                </sheet>
                <footer>
                    <button name="fnimportBonusLines" string="Import Excel" type="object" class="oe_highlight"  />
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

</odoo>
