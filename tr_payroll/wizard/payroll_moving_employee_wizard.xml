<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="payroll_moving_employee_wizzard_view_form" model="ir.ui.view">
        <field name="name">payroll.moving.employee.wizzard.view.form</field>
        <field name="model">payroll.moving.employee.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="from_employee" />
                        <field name="to_employee" />
                    </group>
                </sheet>
                <footer>
                    <button name="submit" string="Submit" type="object" class="btn-primary" />
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

</odoo>
