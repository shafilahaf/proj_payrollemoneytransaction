<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="payroll_import_excel_employee_wizard_view_form" model="ir.ui.view">
        <field name="name">payroll.import.excel.employee.wizard.view.form</field>
        <field name="model">payroll.import.excel.employee.wizard</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="file" />
                    </group>
                </sheet>
                <footer>
                    <button name="upload" string="Upload" type="object" class="btn-primary" />
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

</odoo>
